page {
  background-color: #F4F5F7;
}

.img-icon {
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  vertical-align: text-bottom;
  margin-right: 20rpx;
}

.text-arrows {
  width: 16rpx;
  height: 16rpx;
  display: inline-block;
  border-top: 1px solid #ABB2D0;
  border-right: 1px solid #ABB2D0;
  margin-left: 16rpx;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}
