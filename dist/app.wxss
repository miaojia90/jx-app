.form-main-container {
  padding: 40rpx 0 120rpx 0;
  background-color: #FFF;
  color: #0A1437;
}

.form-main-container .content-box {
  clear: both;
  margin: 0 70rpx;
  margin-bottom: 20rpx;
  font-size: 32rpx;
  padding: 20rpx 20rpx;
  border: 2rpx solid #DBDEE4;
  font-size: 32rpx;
  color: #222C56;
  vertical-align: middle;
  border-radius: 20rpx;
}

.form-main-container .content-box.no-border {
  border: 0px;
}

.form-main-container .content-box .input-view {
  overflow: hidden;
}

.form-main-container .content-box .input-view label {
  float: left;
  line-height: 60rpx;
}

.form-main-container .content-box .input-view input {
  float: left;
  height: 60rpx;
  line-height: 60rpx;
}

.form-main-container .content-box .placeholder {
  font-size: 32rpx;
  color: #222C56;
}

.form-main-container .content-box .checkbox-label {
  vertical-align: middle;
}

.form-main-container .content-box .checkbox-item {
  height: 80rpx;
}

.form-main-container .content-box .radio-view {
  display: inline-block;
}

.form-main-container .content-box .text-view {
  display: inline-block;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #222C56;
  vertical-align: middle;
}

.form-main-container .content-box .textarea-view {
  width: 100%;
  height: 200rpx;
}

.form-main-container .btn-view {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #FFF;
  border-top: 0.5px solid #F4F5F7;
}

.form-main-container .btn-view .btn-save {
  color: #FFF;
  background-color: #8a8a8a;
  font-size: 34rpx;
  height: 110rpx;
  line-height: 110rpx;
  letter-spacing: 8rpx;
  border-radius: 0px;
}

.form-main-container .btn-view .btn-save.no-letter {
  letter-spacing: 0rpx;
}

.form-main-container .btn-view .btn-save.active {
  background-color: #13227a;
  color: #FFF;
}

.main-list-container .list-item-container {
  background-color: #FFF;
}

.main-list-container .list-item-container .item {
  position: relative;
  padding: 30rpx 30rpx;
  border-bottom: 1px solid #dbdee4;
}

.main-list-container .list-item-container .item .user-header-box {
  position: absolute;
  width: 120rpx;
}

.main-list-container .list-item-container .item .user-header-box image {
  width: 100rpx;
  height: 100rpx;
}

.main-list-container .list-item-container .item .content-box {
  margin-left: 130rpx;
}

.main-list-container .list-item-container .item .content-box.spe {
  margin-left: 0;
}

.main-list-container .list-item-container .item .content-box .news-title {
  font-size: 36rpx;
  color: #d81e06;
}

.main-list-container .list-item-container .item .content-box .tel-phone {
  font-size: 36rpx;
  color: #1296db;
}

.main-list-container .list-item-container .item:last-child {
  border-bottom: 0px solid #dbdee4;
}

.main-list-container .list-item-container .item text {
  width: 100%;
  font-size: 32rpx;
  color: #222C56;
}

.main-list-container .list-item-container .item .title {
  width: 100%;
  font-size: 36rpx;
  height: 60rpx;
  line-height: 60rpx;
}

.main-list-container .list-item-container .item .content {
  width: 100%;
  font-size: 36rpx;
  color: #555a73;
  line-height: 60rpx;
}

.main-list-container .list-item-container .item .comment {
  background-color: #e6e6e6;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}

.main-list-container .list-item-container .item .comment .comment-user {
  color: #1296db;
}

.main-list-container .list-item-container .item .desc {
  line-height: 60rpx;
  overflow: hidden;
}

.main-list-container .list-item-container .item .desc .date {
  width: 300rpx;
  float: right;
  text-align: right;
}

.main-list-container .list-item-container .item .desc .see {
  width: 200rpx;
  float: left;
  text-align: left;
}

.fixed-comment-view {
  position: fixed;
  bottom: 140rpx;
  right: 60rpx;
  width: 100rpx;
  background-color: #FFF;
}

.fixed-comment-view image {
  width: 100rpx;
  height: 100rpx;
}

.mui-ellipsis-2 {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.mui-ellipsis-1 {
  overflow: hidden;
  /*超出部分隐藏*/
  white-space: nowrap;
  /*不换行*/
  -o-text-overflow: ellipsis;
  text-overflow: ellipsis;
  /*超出部分文字以...显示*/
}
