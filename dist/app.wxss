.bg {
  background-image: -webkit-gradient(linear, left top, left bottom, from(#faefe7), to(#ffcbd7));
  background-image: -webkit-linear-gradient(top, #faefe7, #ffcbd7);
  background-image: -o-linear-gradient(top, #faefe7, #ffcbd7);
  background-image: linear-gradient(to bottom, #faefe7, #ffcbd7);
  overflow: hidden;
}

.brown-color {
  color: #784344;
}

.pink-color {
  color: #13227a;
}

.white-color {
  color: #fff;
}

.fs24 {
  font-size: 24rpx;
}

.fs28 {
  font-size: 28rpx;
}

.fs32 {
  font-size: 32rpx;
}

.fs36 {
  font-size: 36rpx;
